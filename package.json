{
    "name": "katana",
    "version": "0.1.0",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-ts",
        "serve": "node server/dist/server/src/server.js",
        "watch-node": "nodemon server/dist/server/src/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --coverage --verbose",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc -p ./server",
        "watch-ts": "tsc -w -p ./server",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect server/dist/server/src/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
        "serve:ui": "vue-cli-service serve client/src/main.ts",
        "build:ui": "vue-cli-service build client/src/main.ts"
    },
    "_moduleAliases": {
        "@shared": "server/dist/shared"
    },
    "dependencies": {
        "async": "^3.2.0",
        "axios": "^0.19.2",
        "bcrypt-nodejs": "^0.0.3",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "core-js": "^3.6.5",
        "dotenv": "^8.2.0",
        "errorhandler": "^1.5.1",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.1",
        "express-validator": "^6.4.0",
        "lodash": "^4.17.15",
        "lusca": "^1.6.1",
        "vue": "^2.6.11",
        "vue-router": "^3.1.6",
        "vuetify": "^2.2.25",
        "vuex": "^3.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@types/async": "^3.2.0",
        "@types/bcrypt-nodejs": "^0.0.31",
        "@types/bluebird": "^3.5.30",
        "@types/body-parser": "^1.19.0",
        "@types/compression": "^1.7.0",
        "@types/concurrently": "^5.1.0",
        "@types/errorhandler": "^1.5.0",
        "@types/express": "^4.17.6",
        "@types/express-flash": "0.0.2",
        "@types/express-session": "^1.17.0",
        "@types/jquery": "^3.3.35",
        "@types/lodash": "^4.14.150",
        "@types/lusca": "^1.6.1",
        "@types/node": "^13.13.2",
        "@types/node-sass": "^4.11.0",
        "@types/request": "^2.48.4",
        "@types/request-promise": "^4.1.46",
        "@types/shelljs": "^0.8.7",
        "@types/supertest": "^2.0.8",
        "@vue/cli-plugin-babel": "~4.3.1",
        "@vue/cli-plugin-router": "~4.3.1",
        "@vue/cli-plugin-typescript": "~4.3.1",
        "@vue/cli-plugin-vuex": "~4.3.1",
        "@vue/cli-service": "~4.3.1",
        "concurrently": "^5.2.0",
        "module-alias": "^2.2.2",
        "node-sass": "^4.14.0",
        "nodemon": "^2.0.3",
        "regenerator-runtime": "^0.13.5",
        "sass": "^1.26.5",
        "sass-loader": "^8.0.2",
        "shelljs": "^0.8.4",
        "supertest": "^4.0.2",
        "ts-node": "^8.9.0",
        "typescript": "^3.8.3",
        "vue-cli-plugin-vuetify": "~2.0.5",
        "vue-template-compiler": "^2.6.11",
        "vuetify-loader": "^1.4.3"
    }
}